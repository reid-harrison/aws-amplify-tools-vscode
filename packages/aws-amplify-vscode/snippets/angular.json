{
    "Angular/Ionic amplify-photo-picker": {
        "prefix": "<amplify-photo-picker",
        "body": [
            "<amplify-photo-picker",
            "\t(picked)=\"onImagePicked($event)\"",
            "\t(loaded)=\"onImageLoaded($event)\">",
            "</amplify-photo-picker>"
        ]
    },
    "Angluar onImagePicked": {
        "prefix": "onImagePicked",
        "body": [
            "onImagePicked( file ) {\n",
                "\tlet key = `pics/${file.name}`;\n",
                "\tthis.amplify.storage().put( key, file, {",
                    "\t\t'level': 'private',",
                    "\t\t'contentType': file.type",
                "\t})",
                "\t.then (result => console.log('uploaded: ', result))",
                "\t.catch(err => console.log('upload error: ', err));\n",
                "}"
        ]
    },
