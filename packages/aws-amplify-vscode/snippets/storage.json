{
    "Amplify Manual Storage Configuration": {
        "prefix": "Storage:",
        "body": [
            "Storage: {",
            "\tbucket: '', // REQUIRED - Amazon  S3 bucket",
            "\tregion: 'XX-XXXX-X' // OPTIONAL - Amazon service region",
            "}"
        ]
    },
    "Manual Storage Configuration": {
        "prefix": "Storage.configure",
        "body": [
            "Storage.configure({",
                "\tbucket: // Your bucket ARN;",
                "\tregion: // Specify the region your bucket was created in;",
                "\tidentityPoolId: // Specify your identityPoolId for Auth and Unauth access to your bucket;",
            "});"
        ]
    },
    "Storage Get": {
        "prefix": "Storage.get",
        "body": [
            "Storage.get('test.txt', { level: 'private' })",
            "\t.then ( (result) => { ",
            "\t\tconsole.log(result); ", 
            "\t})",
            "\t.catch( (err) => { ",
            "\t\tconsole.log(err);",
            "\t});"
        ]
    },
    "Storage Put": {
        "prefix": "Storage.put",
        "body": [
            "Storage.put('test.txt', 'Protected Content', {",
            "\tlevel: 'private',",
            "\tcontentType: 'text/plain'",
            "})",
            ".then ( (result) => { ",
            "\tconsole.log(result); ", 
            "})",
            ".catch( (err) => { ",
            "\tconsole.log(err);",
            "});"
        ]
    },
